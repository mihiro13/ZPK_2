{
    "hud_actionbar_text": {
        "$base_text": "$actionbar_text",
        "visible": false
    },
    "label": {
        "$base_text": "$actionbar_text",
        "$text": "$base_text",
        "type": "image",
        "size": [
            "100%c + 12px",
            "100%c + 5px"
        ],
        "offset": [
            -6,
            0
        ],
        "texture": "textures/ui/hud_tip_text_background",
        "alpha": "@hud.anim_actionbar_text_background_alpha_out",
        "anchor_from": "top_left",
        "anchor_to": "top_left",
        "controls": [
            {
                "actionbar_message": {
                    "type": "label",
                    "shadow": true,
                    "anchor_from": "top_left",
                    "anchor_to": "top_left",
                    "color": "$tool_tip_text",
                    "layer": 31,
                    "text": "$text",
                    "localize": false,
                    "text_alignment": "center",
                    "alpha": "@hud.anim_actionbar_text_alpha_out"
                }
            }
        ],
        "visible": "(not ($text = ''))"
    },

    "label_x@hud.label": {
        "$text": "('%.30s' * $base_text)",
        "type": "image",
        "offset": "$x_offset"
    },
    "label_y@hud.label": {
        "$text": "(('%.60s' * $base_text) - ('%.30s' * $base_text))",
